{
  "name": "api-node",
  "version": "0.1.0",
  "description": "This project is a sample of an api in node",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "test": "NODE_ENV=test LOG_LEVEL=OFF node --experimental-vm-modules node-modules/jest/bin/jest.js",
    "native": "npm run transpile && npm run compile",
    "transpile": "esbuild --platform=node --bundle --outfile=bin/native.cjs src/server.js",
    "compile": "pkg --no-bytecode --public --targets node16-linux-x64 --public-packages '*' --compress brotli --out-path bin bin/native.cjs"
  },
  "author": "Valentin CONAN <valentin.conan@gmail.com>",
  "devDependencies": {
    "@jest/globals": "28.1.0",
    "jest": "28.1.0",
    "jest-junit": "13.2.0",
    "pkg": "5.8.1",
    "esbuild": "0.17.11"
  },
  "dependencies": {
    "npm": "8.11.0",
    "log4js": "6.4.7",
    "cors": "2.8.5",
    "express": "4.18.1"
  },
  "pre-commit": []
}
